{
  "name": "short-links-mern",
  "version": "1.0.0",
  "description": "mern stack",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Markus-cpu/short-links-mern.git"
  },
  "keywords": [
    "react",
    "express",
    "mongoose",
    "node"
  ],
  "author": "Markus-cpu <markbass82@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Markus-cpu/short-links-mern/issues"
  },
  "homepage": "https://github.com/Markus-cpu/short-links-mern#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "http-proxy-middleware": "^1.0.6",
    "nodemon": "^2.0.6"
  }
